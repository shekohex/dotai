{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "share": "manual",
  "autoupdate": false,
  "username": "shekohex",
  "plugin": [
    "opencode-pty@0.2.3"
  ],
  "small_model": "opencode/gpt-5-nano",
  "agent": {
    "explore": {
      "model": "kimi-for-coding/k2p5",
      "variant": "high",
      "color": "#4285F4"
    },
    "plan": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "build": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high",
      "color": "#FF6B35"
    },
    "gsd-planner": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-plan-checker": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-phase-researcher": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-roadmapper": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-project-researcher": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-research-synthesizer": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-codebase-mapper": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "gsd-executor": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high",
      "color": "#FF6B35"
    },
    "gsd-debugger": {
      "model": "openai/gpt-5.3-codex",
      "variant": "high",
      "color": "#FF6B35"
    },
    "gsd-verifier": {
      "model": "kimi-for-coding/k2p5",
      "variant": "high",
      "color": "#4285F4"
    },
    "gsd-integration-checker": {
      "model": "kimi-for-coding/k2p5",
      "variant": "high",
      "color": "#4285F4"
    },
    "gsd-set-profile": {
      "model": "kimi-for-coding/k2p5",
      "variant": "high",
      "color": "#4285F4"
    },
    "gsd-settings": {
      "model": "kimi-for-coding/k2p5",
      "variant": "high",
      "color": "#4285F4"
    },
    "gsd-set-model": {
      "model": "kimi-for-coding/k2p5",
      "variant": "high",
      "color": "#4285F4"
    }
  },
  "provider": {
    "google": {
      "npm": "@ai-sdk/google",
      "name": "Google (Bigboss)",
      "api": "http://100.100.1.116:4000/v1beta",
      "options": {
        "baseURL": "http://100.100.1.116:4000/v1beta"
      }
    },
    "openai": {
      "name": "OpenAI (Bigboss)",
      "api": "http://100.100.1.116:4000/v1",
      "options": {
        "baseURL": "http://100.100.1.116:4000/v1"
      },
      "models": {
        "gpt-5.2": {
          "limit": {
            "context": 272000,
            "output": 128000
          }
        },
        "gpt-5.3-codex": {
          "limit": {
            "context": 272000,
            "output": 128000
          }
        }
      }
    },
    "zai": {
      "name": "Z.AI (Bigboss)",
      "api": "http://100.100.1.116:4000/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://100.100.1.116:4000/v1"
      }
    },
    "anthropic": {
      "name": "Anthropic (Bigboss)",
      "api": "http://100.100.1.116:4000/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://100.100.1.116:4000/v1"
      }
    },
    "kimi-for-coding": {
      "name": "Kimi (Bigboss)",
      "api": "http://100.100.1.116:4000/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://100.100.1.116:4000/v1"
      }
    }
  },
  "permission": {
    "bash": {
      "git push*": "ask",
      "rm -rf*": "ask",
      "*": "allow"
    },
    "read": {
      "*": "allow",
      "*.env": "deny",
      "*.env.example": "allow"
    },
    "external_directory": {
      "*": "ask",
      "/tmp": "allow",
      "/tmp/*": "allow",
      "/home/coder/*": "allow",
      "/private/tmp": "allow",
      "/private/tmp/*": "allow",
      "env:$TMPDIR": "allow",
      "env:$TMPDIR/*": "allow"
    }
  },
  "tui": {
    "diff_style": "auto",
    "scroll_speed": 6,
    "scroll_acceleration": {
      "enabled": true
    }
  },
  "formatter": false,
  "experimental": {
    "batch_tool": false
  },
  "mcp": {}
}
