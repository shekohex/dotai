{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "enabled": true
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app",
      "enabled": false
    },
    "firecrawl": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "firecrawl-mcp"
      ],
      "env": {
        "FIRECRAWL_API_KEY": "fc-free",
        "FIRECRAWL_API_URL": "http://192.168.1.121:3000",
        "FIRECRAWL_RETRY_MAX_ATTEMPTS": "10",
        "FIRECRAWL_RETRY_INITIAL_DELAY": "500"
      },
      "enabled": true
    },
    "gemini": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "gemini-mcp-tool"
      ],
      "enabled": false
    },
    "taskmaster-ai": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "enabled": false
    },
    "zai": {
      "enabled": true,
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@z_ai/mcp-server"
      ],
      "env": {
        "Z_AI_API_KEY": "your_api_key",
        "Z_AI_MODE": "ZAI"
      }
    }
  }
}
