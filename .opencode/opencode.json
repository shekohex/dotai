{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "autoshare": false,
  "autoupdate": false,
  "plugin": [
    "opencode-openai-codex-auth@3.1.0"
  ],
  "provider": {
    "bigboss": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "BigBoss",
      "options": {
        "baseURL": "http://192.168.1.120:8080/v1"
      },
      "models": {
        "qwen3-coder": {
          "name": "Qwen 3 Coder",
          "reasoning": false,
          "release_date": "2025-07-31",
          "cost": {
            "input": 0.3,
            "output": 1.2
          },
          "temperature": false
        }
      }
    },
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": [
          "reasoning.encrypted_content"
        ],
        "store": false
      },
      "models": {
        "gpt-5-codex-low": {
          "name": "GPT 5 Codex Low (OAuth)",
          "limit": {
            "context": 400000,
            "output": 128000
          },
          "options": {
            "reasoningEffort": "low",
            "reasoningSummary": "auto",
            "textVerbosity": "medium",
            "include": [
              "reasoning.encrypted_content"
            ],
            "store": false
          }
        },
        "gpt-5-codex-medium": {
