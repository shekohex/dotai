{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "share": "manual",
  "autoupdate": false,
  "username": "shekohex",
  "plugin": [
    "opencode-pty@0.1.5"
  ],
  "small_model": "opencode/gpt-5-nano",
  "agent": {
    "explore": {
      "model": "google/gemini-3-flash-preview",
      "variant": "medium",
      "color": "#4285F4"
    },
    "plan": {
      "model": "openai/gpt-5.2",
      "variant": "high",
      "color": "#10A37F"
    },
    "build": {
      "model": "openai/gpt-5.3-codex",
      "variant": "medium",
      "color": "#FF6B35"
    }
  },
  "provider": {
    "google": {
      "npm": "@ai-sdk/google",
      "name": "Google (Bigboss)",
      "api": "http://192.168.1.116:4000/v1beta",
      "options": {
        "baseURL": "http://192.168.1.116:4000/v1beta"
      }
    },
    "openai": {
      "name": "OpenAI (Bigboss)",
      "api": "http://192.168.1.116:4000/v1",
      "options": {
        "baseURL": "http://192.168.1.116:4000/v1"
      },
      "models": {
        "gpt-5.2": {
          "limit": {
            "context": 272000,
            "output": 128000
          }
        },
        "gpt-5.3-codex": {
          "limit": {
            "context": 272000,
            "output": 128000
          }
        }
      }
    },
    "zai": {
      "name": "Z.AI (Bigboss)",
      "api": "http://192.168.1.116:4000/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://192.168.1.116:4000/v1"
      }
    },
    "anthropic": {
      "name": "Anthropic (Bigboss)",
      "api": "http://192.168.1.116:4000/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://192.168.1.116:4000/v1"
      }
    },
    "kimi-for-coding": {
      "name": "Kimi (Bigboss)",
      "api": "http://192.168.1.116:4000/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "http://192.168.1.116:4000/v1"
      }
    }
  },
  "permission": {
    "bash": {
      "git push*": "ask",
      "rm -rf*": "ask",
      "*": "allow"
    }
  },
  "tui": {
    "diff_style": "auto",
    "scroll_acceleration": {
      "enabled": true
    }
  },
  "experimental": {
    "batch_tool": false
  },
  "mcp": {}
}
