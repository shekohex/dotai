{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "share": "manual",
  "autoupdate": false,
  "username": "shekohex",
  "plugin": [
    "opencode-google-antigravity-auth@0.2.14",
    "opencode-pty@0.1.4"
  ],
  "small_model": "opencode/gpt-5-nano",
  "provider": {
    "google": {
      "npm": "@ai-sdk/google",
      "models": {
        "gemini-claude-opus-4-5-thinking": {
          "id": "gemini-claude-opus-4-5-thinking",
          "name": "Claude Opus 4.5",
          "release_date": "2025-11-24",
          "reasoning": true,
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": [
              "text",
              "image",
              "pdf"
            ],
            "output": [
              "text"
            ]
          },
          "variants": {
            "low": {
              "options": {
                "thinkingConfig": {
                  "thinkingBudget": 4000,
                  "includeThoughts": true
                }
              }
            },
            "medium": {
              "options": {
                "thinkingConfig": {
                  "thinkingBudget": 16000,
                  "includeThoughts": true
                }
              }
            },
            "high": {
              "options": {
                "thinkingConfig": {
                  "thinkingBudget": 32000,
                  "includeThoughts": true
                }
              }
            }
          }
        }
      }
    },
    "openai": {
      "name": "OpenAI (Bigboss)",
      "api": "https://ai-gateway.0iq.xyz/v1",
      "options": {
        "baseURL": "https://ai-gateway.0iq.xyz/v1"
      }
    }
  },
  "permission": {
    "bash": {
      "git push *": "ask",
      "rm -rf *": "ask",
      "*": "allow"
    }
  },
  "tui": {
    "diff_style": "auto",
    "scroll_acceleration": {
      "enabled": true
    }
  },
  "experimental": {
    "batch_tool": false
  },
  "mcp": {}
}
