{
  "mcpServers": {
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "enabled": false
    },
    "grep": {
      "type": "http",
      "url": "https://mcp.grep.app",
      "enabled": false
    },
    "firecrawl": {
      "type": "http",
      "url": "http://192.168.1.121:8081/mcp",
      "enabled": true
    },
    "gemini": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "gemini-mcp-tool"
      ],
      "enabled": false
    },
    "taskmaster-ai": {
      "command": "bun",
      "args": [
        "x",
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "enabled": false
    },
    "zai": {
      "enabled": false,
      "command": "bun",
      "args": [
        "x",
        "-y",
        "@z_ai/mcp-server"
      ],
      "env": {
        "Z_AI_API_KEY": "your_api_key",
        "Z_AI_MODE": "ZAI"
      }
    },
    "web-search": {
      "enabled": false,
      "type": "http",
      "url": "https://api.z.ai/api/mcp/web_search_prime/mcp",
      "headers": {
        "Authorization": "Bearer your_api_key"
      }
    }
  }
}
