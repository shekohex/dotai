{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "share": "manual",
  "autoupdate": false,
  "username": "shekohex",
  "plugin": [
    "opencode-openai-codex-auth@4.0.2",
    "opencode-gemini-auth@1.2.0",
    "opencode-skills@0.1.1"
  ],
  "model": "zai/glm-4.6",
  "small_model": "opencode/big-pickle",
  "provider": {
    "zai": {
      "name": "Z.AI (Anthropic)",
      "env": [
        "ZHIPU_API_KEY",
        "ZAI_API_KEY"
      ],
      "npm": "@ai-sdk/anthropic",
      "api": "https://api.z.ai/api/anthropic/v1"
    },
    "google": {
      "models": {
        "gemini-3-pro-high": {
          "id": "gemini-3-pro-preview",
          "name": "Gemini 3 Pro (High)",
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "high",
              "includeThoughts": true
            }
          }
        },
        "gemini-3-pro-medium": {
          "id": "gemini-3-pro-preview",
          "name": "Gemini 3 Pro (Medium)",
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "medium",
              "includeThoughts": true
            }
          }
        },
        "gemini-3-pro-low": {
          "id": "gemini-3-pro-preview",
          "name": "Gemini 3 Pro (Low)",
          "options": {
            "thinkingConfig": {
              "thinkingLevel": "low",
              "includeThoughts": true
            }
          }
        },
        "gemini-2.5-pro-high": {
          "id": "gemini-2.5-pro-preview",
          "name": "Gemini 2.5 Pro (High)",
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 8192,
              "includeThoughts": true
            }
          }
        },
        "gemini-2.5-pro-medium": {
          "id": "gemini-2.5-pro-preview",
          "name": "Gemini 2.5 Pro (Medium)",
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 4096,
              "includeThoughts": true
            }
          }
        },
        "gemini-2.5-pro-low": {
          "id": "gemini-2.5-pro-preview",
          "name": "Gemini 2.5 Pro (Low)",
          "options": {
            "thinkingConfig": {
              "thinkingBudget": 2048,
              "includeThoughts": true
            }
          }
        }
      }
    },
    "openai": {
      "options": {
        "reasoningEffort": "medium",
        "reasoningSummary": "auto",
        "textVerbosity": "medium",
        "include": [
          "reasoning.encrypted_content"
        ],
        "store": false
      },
      "models": {
        "gpt-5.1-codex-low": {
          "name": "GPT 5.1 Codex Low (OAuth)",
          "limit": {
            "context": 272000,
            "output": 128000
          },
          "options": {
            "reasoningEffort": "low",
            "reasoningSummary": "auto",
            "textVerbosity": "medium",
            "include": [
              "reasoning.encrypted_content"
