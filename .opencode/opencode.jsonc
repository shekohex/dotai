{
  "$schema": "https://opencode.ai/config.json",
  "theme": "catppuccin",
  "share": "manual",
  "autoupdate": false,
  "username": "shekohex",
  "plugin": [
    "opencode-pty@0.1.4"
  ],
  "small_model": "opencode/gpt-5-nano",
  "provider": {
    "google": {
      "npm": "@ai-sdk/google",
      "name": "Google (Bigboss)",
      "api": "https://ai-gateway.0iq.xyz/v1beta",
      "options": {
        "baseURL": "https://ai-gateway.0iq.xyz/v1beta"
      }
    },
    "openai": {
      "name": "OpenAI (Bigboss)",
      "api": "https://ai-gateway.0iq.xyz/v1",
      "options": {
        "baseURL": "https://ai-gateway.0iq.xyz/v1"
      }
    },
    "zai": {
      "name": "Z.AI (Bigboss)",
      "api": "https://ai-gateway.0iq.xyz/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "https://ai-gateway.0iq.xyz/v1"
      }
    },
    "anthropic": {
      "name": "Anthropic (Bigboss)",
      "api": "https://ai-gateway.0iq.xyz/v1",
      "npm": "@ai-sdk/anthropic",
      "options": {
        "baseURL": "https://ai-gateway.0iq.xyz/v1"
      }
    }
  },
  "permission": {
    "bash": {
      "git push *": "ask",
      "rm -rf *": "ask",
      "*": "allow"
    }
  },
  "tui": {
    "diff_style": "auto",
    "scroll_acceleration": {
      "enabled": true
    }
  },
  "experimental": {
    "batch_tool": false
  },
  "mcp": {}
}
